~/development/assignments/terminal-practice/dir-two $ ls
index.html  main.js
~/development/assignments/terminal-practice/dir-two $ code .
~/development/assignments/terminal-practice/dir-two $ vi index.html
~/development/assignments/terminal-practice/dir-two $ vi index.html
~/development/assignments/terminal-practice/dir-two $